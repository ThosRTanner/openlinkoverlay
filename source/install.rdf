<?xml version='1.0' encoding='utf-8'?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:em="http://www.mozilla.org/2004/em-rdf#">
  <Description about="urn:mozilla:install-manifest">
    <em:type>2</em:type>
    <em:id>openlinkoverlay@thosrtanner.github.com</em:id>
    <em:name>Open link menu</em:name>
    <em:version>2.0.0.0 (pre-release)</em:version>
    <em:creator>ThosRTanner</em:creator>
    <em:description>Adds more tab/window opening options to the context menu for links and images.</em:description>
    <em:homepageURL>https://github.com/ThosRTanner/openlinkoverlay</em:homepageURL>

    <em:developer>Anton Prowse</em:developer>
    <em:developer>ThosRTanner</em:developer>

    <em:contributor>Off JustOff &lt;Off.Just.Off@gmail.com&gt;</em:contributor>

    <em:translator>banthaz (sv-SE)</em:translator>
    <em:translator>jericane1 (fr-FR)</em:translator>
    <em:translator>Vladimir Zhirov (ru-RU)</em:translator>
    <em:translator>ReinekeFux (de-DE)</em:translator>

    <em:localized>
      <Description>
        <em:locale>de-DE</em:locale>
        <em:name>Open link menu</em:name>
        <em:description>Fügt dem Kontextmenü weitere Optionen zum Öffnen von Links in Tabs/Fenstern hinzu.</em:description>
      </Description>
    </em:localized>
    <em:localized>
      <Description>
        <em:locale>fr-FR</em:locale>
        <em:name>Ouvrir le lien ...</em:name>
        <em:description>Ajoutez plus d'options d'ouverture d'onglet/fenêtre au menu contextuel, pour les liens et les images.</em:description>
      </Description>
    </em:localized>
    <em:localized>
      <Description>
        <em:locale>ru-RU</em:locale>
        <em:name>Открыть ссылку ...</em:name>
        <em:description>Добавляет в контекстное меню ссылок и изображений больше команд, позволяющих открывать ссылки в окнах/вкладках.</em:description>
      </Description>
    </em:localized>
    <em:localized>
      <Description>
        <em:locale>sv-SE</em:locale>
        <em:name>Öppna länk ...</em:name>
        <em:description>Lägger till fler alternativ för att öppna i flikar/fönster i snabbmenyerna för länkar och bilder.</em:description>
      </Description>
    </em:localized>
    <em:optionsURL>chrome://openlink/content/openlinkSettings.xul</em:optionsURL>

    <!-- Palemoon -->
    <em:targetApplication>
      <Description>
        <em:id>{8de7fcbb-c55c-4fbe-bfc5-fc555c87dbc4}</em:id>
        <em:minVersion>28.8.4</em:minVersion>
        <em:maxVersion>29.*</em:maxVersion>
      </Description>
    </em:targetApplication>

    <!-- firefox/basilisk -->
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>4.0</em:minVersion>
        <em:maxVersion>56.*</em:maxVersion>
      </Description>
    </em:targetApplication>
  </Description>
</RDF>
